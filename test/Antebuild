
class TestProject(cpp.Executable):
    SourceGroups = {
        "Folder 1": [ 
            "src/mainexe.cpp", 
            "src/foo1.cpp", 
            "src/bar1.cpp", 
        ],

        "Folder 2": [
            "src/main2.cpp", 
            "src/foo2.cpp", 
            "src/bar2.cpp", 
        ],

        "Header Files": [
            "src/bar1.h",
            "src/bar2.h",
            "src/foo1.h",
            "src/foo2.h",
            "src/mainexe.h",
            "src/main2.h",
        ],
    }

    PrecompiledHeaderOptions = {
        "Prefix": "src",
        "Include": "pch.h",
        "CompileFrom": "src/pch.cpp",
    }
